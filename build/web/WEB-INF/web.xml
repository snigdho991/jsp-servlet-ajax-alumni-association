<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>check</servlet-name>
        <servlet-class>CheckAvailability</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SliderUploadServlet</servlet-name>
        <servlet-class>SliderUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aboutImage</servlet-name>
        <servlet-class>aboutImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertEvent</servlet-name>
        <servlet-class>insertEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailController</servlet-name>
        <servlet-class>EmailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertNews</servlet-name>
        <servlet-class>insertNews</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newsImage</servlet-name>
        <servlet-class>newsImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileImage</servlet-name>
        <servlet-class>profileImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>check</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SliderUploadServlet</servlet-name>
        <url-pattern>/SliderUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aboutImage</servlet-name>
        <url-pattern>/aboutImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertEvent</servlet-name>
        <url-pattern>/insertEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailController</servlet-name>
        <url-pattern>/EmailController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertNews</servlet-name>
        <url-pattern>/insertNews</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newsImage</servlet-name>
        <url-pattern>/newsImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileImage</servlet-name>
        <url-pattern>/profileImage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
